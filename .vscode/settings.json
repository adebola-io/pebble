{
    // "editor.fontFamily": "'Fira Code', 'Source Code Pro', 'Consolas', 'Courier New', monospace",
    // "editor.fontFamily": "'Source Code Pro', 'Consolas', 'Courier New', monospace",
    "editor.fontFamily": "'Consolas', 'Courier New', monospace",
    "workbench.colorTheme": "Slack Theme Dark Mode",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        "editorInlayHint.background": "#00000000",
        "editorInlayHint.parameterBackground": "#00000000",
        "editorInlayHint.typeBackground": "#00000000"
    },
    "editor.tabSize": 4,
    "prettier.tabWidth": 4,
    "editor.inlayHints.fontSize": 10,
    "rust-analyzer.inlayHints.typeHints.enable": true,
    "rust-analyzer.inlayHints.parameterHints.enable": true,
    "rust-analyzer.inlayHints.closingBraceHints.enable": true,
    "editor.tokenColorCustomizations": {
        "[Red]": {
            "textMateRules": [
                {
                    "scope": "variable",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": ["entity.name.function", "meta.function-call"],
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": ["comment"],
                    "settings": {
                        "foreground": "#744c4c",
                        "fontStyle": ""
                    }
                }
            ]
        }
    },
    "material-icon-theme.files.associations": {
        "*.peb": "typescript",
        "pile.types": "/extensions/vscode-pebble/icons/pile-types",
        "pile.json": "/extensions/vscode-pebble/icons/json-plain"
    },
    "vsicons.associations.files": [],
    "material-icon-theme.folders.associations": {
        "piles": "piles",
        "*.pile": "packages"
    },
    "json.schemas": [
        {
            "fileMatch": ["/pile.json"],
            "url": "/design/config.schema.json"
        }
    ]
}
